{
  "seed": 42,
  "dataset": "pathmnist",
  "num_virtual_clients": 10,
  "client_fraction": 0.3,
  "rounds": 10,
  "batch_size": 64,
  "val_batch_size": 256,
  "local_epochs": 5,
  "lr": 0.0008,
  "weight_decay": 0.0001,
  "teacher_epochs": 10,
  "task_mode": "class-episodes",
  "classes_per_task": 3,
  "tasks_per_client": 3,
  "use_ewc": true,
  "ewc_lambda": 100.0,
  "ewc_gamma": 0.97,
  "ewc_max_batches": 100,
  "use_adapters": true,
  "adapter_inner_steps": 3,
  "adapter_lr": 0.0005,
  "prune_for_deploy": true,
  "deploy_ft_epochs": 5,
  "quantize_for_deploy": true,
  "num_workers": 0,
  "pin_memory": false,
  "persistent_workers": false,
  "prefetch_factor": 2,
  "use_amp": true,
  "amp_dtype": "bf16",
  "bigger_batch_on_gpu": true,
  "gpu_batch_multiplier": 2,
  "limit_train_batches": 0,
  "amp_in_fisher": false,
  "strategies_to_run": [
    "fedavg",
    "fedavgm"
  ],
  "dp_enabled": false,
  "dp_max_grad_norm": 1.0,
  "dp_noise_multiplier": 1.0,
  "dp_target_delta": 1e-05,
  "min_channels_per_layer": 8,
  "prune_step": 0.05,
  "latency_tolerance_ms": 2.0,
  "grad_accum_steps_small_device": 1,
  "root_dir": "artifacts_fed_flwr_ray",
  "teacher_dir": "Teacher",
  "server_dir": "Server",
  "client_metrics_dir": "ClientMetrics"
}